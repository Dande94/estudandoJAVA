package controle;

import java.util.Scanner;

public class DesafioWhile {
	public static void main(String[] args) {
		
		Scanner entrada = new Scanner(System.in);

		boolean verificador = false;
		double notas;
		int total = 0;
		do {
			if(notas <0 || notas >10) {
				System.out.println("Nota Invalida, digite novamente");
			}
			
			
		}while(!verificador);

		entrada.close();

		
	}
	
}
