package controle;

import java.util.Scanner;

public class DesafioDiaSemana {
	public static void main(String[] args) {
//minha resposta
		Scanner entrada = new Scanner(System.in);
		String dia = entrada.next();
		
		if(dia.equals("domingo")) {
			System.out.println(1);
		}
		
		
		entrada.close(); 
		
	}
}
