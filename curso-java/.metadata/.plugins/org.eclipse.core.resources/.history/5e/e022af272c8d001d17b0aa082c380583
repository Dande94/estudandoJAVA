package oo.composicao;

public class Motor {
	
	Carro carro = new Carro();//Relação bidirecional -> não aconselhavél 
	
	boolean ligado = false;
	double fatorInjecao = 1;
	
	Motor(Carro carro){
		this.carro = carro;
	}
	
	int giros() {
		if(!ligado) {
			return 0;
		}else {
			return (int) Math.round(fatorInjecao * 3000);
		}
		
	}
}
