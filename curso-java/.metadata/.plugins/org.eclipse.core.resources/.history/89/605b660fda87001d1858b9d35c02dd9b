package classe;

public class ValorVsReferencia {
	public static void main(String[] args) {
		//atribuição por valor(primitivo)
		double a  = 2;
		double b = a;
		a++;
		b--;
		System.out.println(a + " " + b);
		
		//atribuição por referencia(objeto)
		Data d1 = new Data(1, 6, 2022);
		Data d2 = d1;
		
	}
	

}
